{
    "name": "oclock/twilio-messenger",
    "description": "this is sameple packages created usign twilio php sdk",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "twilio",
        "laravel",
        "php"
    ],
    "authors": [
        {
            "name": "Oclock Software",
            "email": "manikandan.a@oclocksolutions.com"
        }
